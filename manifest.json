{
    "name": "Shiven's Ad Blocker",
    "version": "1.0",
    "description": "Blocks ads on the internet",
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "<all_urls>",
        "webNavigation",
        "tabs",
        "windows"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["block_location.js"]
        }
    ],
    "icons": {
        "16": "Icons/16x16.png",
        "48": "Icons/48x48.png",
        "128": "Icons/128x128.png"
    },
    "manifest_version": 2
}
